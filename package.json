{
    "name": "eslint-config-wotusay",
    "version": "1.0.8",
    "description": "Eslint config for angular 12+",
    "main": "index.js",
    "homepage": "https://github.com/Wotusay/eslint-wotusay#readme",
    "author": "Wotusay",
    "license": "MIT",
    "keywords": [
        "javascript",
        "ecmascript",
        "eslint",
        "lint",
        "config",
        "eslint-config",
        "angular"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Wotusay/eslint-wotusay"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "dist"
    ],
    "scripts": {
        "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
        "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
        "validate": "npm run lint --silent"
    },
    "devDependencies": {
        "prettier": "^2.6.0",
        "typescript": "^4.6.2",
        "@angular-eslint/builder": "13.2.1",
        "@angular-eslint/eslint-plugin": "13.2.1",
        "@angular-eslint/eslint-plugin-template": "13.2.1",
        "@angular-eslint/template-parser": "12.6.1",
        "@typescript-eslint/eslint-plugin": "5.23.0",
        "@typescript-eslint/parser": "5.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "@types/eslint": "^8.4.1",
        "@types/node": "^17.0.23",
        "eslint": "^8.15.0"
    },
    "peerDependencies": {
        "prettier": "^2.6.0",
        "@angular-eslint/builder": "13.2.1",
        "@angular-eslint/eslint-plugin": "13.2.1",
        "@angular-eslint/eslint-plugin-template": "13.2.1",
        "@angular-eslint/template-parser": "12.6.1",
        "@typescript-eslint/eslint-plugin": "5.23.0",
        "@typescript-eslint/parser": "5.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "@types/eslint": "^8.4.1",
        "@types/node": "^17.0.23",
        "eslint": "^8.15.0"
    }
